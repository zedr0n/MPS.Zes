namespace ZES.Tests.Domain.Commands
{
  public class AddRecord : Command
  {
    public double RecordValue
    {
       get; 
       set;
    }  
    public AddRecord() 
    {
    }  
    public AddRecord(string target, double recordValue) : base(target) 
    {
      RecordValue = recordValue;
    }
  }
}

