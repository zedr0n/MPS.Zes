namespace ZES.Tests.Domain.Commands
{
  public class CreateRecord : Command, ICreateCommand
  {
    public string Target
    {
       get; 
       set;
    }  
    public CreateRecord() 
    {
    }  
    public CreateRecord(string target) : base(target) 
    {
      Target = target;
    }  
    public string Target
    {
       get; 
       set;
    }
  }
}

