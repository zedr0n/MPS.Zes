namespace ZES.Tests.Domain.Queries
{
  public class RootInfoQuery : Query<RootInfo>
  {
    public string Id
    {
       get; 
       set;
    }  
    public RootInfoQuery() 
    {
    }  
    public RootInfoQuery(string id) 
    {
      Id = id;
    }
  }
}

